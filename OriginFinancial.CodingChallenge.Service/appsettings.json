{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "EnvironmentConfiguration": {
    "EnvironmentVariables": [
      {
        "EnvironmentName": "ORIGIN_ENVIRONMENT",
        "Contexts": {
          "ConnStringsName": "CONN_STRINGS",
          "DatabaseNames": [
            "ORIGIN_MAIN_DATABASE",
            "ORIGIN_AUTH_DATABASE"
          ]
        },
        "Security": {
          "SecurityName": "JWT_SECURITY",
          "SecurityValue": "JWT_KEY"
        }
      }
    ]
  }
}
